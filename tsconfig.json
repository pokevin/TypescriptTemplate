{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "allowJs": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    }
}